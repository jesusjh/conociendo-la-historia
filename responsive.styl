@media screen and (min-width:768px)
	body
		margin 0 auto
		max-width 1200px

		header
			width 95%

			nav
				font-size 1.5em
				width 90%

			#nav
				width 70%

		#contenedor
			display inline-blok
			width 95%

			#texto
				backgound #ddd
				display inline-blok
				font-size 1.2em
				margin 0.3em
				padding 0.3em 0.3em
				text-align left
				width 70%


			#item
				display inline-blok
				font-size 1.3em
				float right
				width 40%

			#item-texto
				float left
				display inline-blok
				font-size 1.3em
				width 50% 

			#pie
				width 100%

				#enlaces
					margin-top 1em
					position relative
					float left
					width 50%

					a 
						border-right 1px solid white
						font-size 0.7em
						padding 0.2em

				#imagenes
					float right
					display inline-blok
					margin 0.3em
					width 30%

					.img 
						width 10%
				
